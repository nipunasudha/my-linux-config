alias ..="cd .."
alias ...="cd ../.."
alias md="mkdir"
alias goo="googler"
alias c="clear"
alias install="sudo apt install"
alias update="sudo apt update"
alias search="sudo apt search"
alias remove="sudo apt remove"
alias showmenu="UBUNTU_MENUPROXY=0"
alias syn="cd ~/synopsys"
alias gita="git add ."
alias gitc="git commit -a -m"
alias gits="git status"
alias gitt="git add . && git commit -a -m"
alias qm4="qmake-qt4"
alias refresh="source ~/.bashrc"
alias q="qm4 && make"
alias qp="qm4 -project"
alias qc="make clean && rm Makefile && find . -maxdepth 1 -type f -executable -exec rm {} +"
alias alias-e="vim ~/.bash_aliases"
alias alias-s="source ~/.bash_aliases"
alias sym-run="php app/console server:run"
alias qq="clear && q && ./out"
alias reload-sound="pulseaudio -k && sudo alsa force-reload"
alias subs="subliminal download --single -l en"
alias vimrc="vim ~/.vimrc"
alias go="gnome-open"
alias git-cred="git config credential.helper store"
alias ll="ls -l"
alias phpstorm="~/software/PhpStorm-172.3317.83/bin/phpstorm.sh"
